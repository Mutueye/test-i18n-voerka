{
  "name": "test-i18n-voerka",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "packageManager": "pnpm@7.18.1",
  "scripts": {
    "prepare": "simple-git-hooks",
    "dev": "vite --mode development",
    "dev:prod": "vite --mode production",
    "dev:local": "vite --mode local",
    "build": "vue-tsc && vite build --mode production",
    "preview": "vite preview",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "lint": "eslint --ext .js,.ts,.vue --ignore-path .gitignore --fix src/",
    "lint:check": "eslint --ext .js,.ts,.vue --ignore-path .gitignore src/",
    "lint:style": "stylelint ./src/**/*.{vue,css,scss} --fix",
    "lint:style:check": "stylelint ./src/**/*.{vue,css,scss}",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:update": "vitest --update",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@iconify-json/line-md": "^1.1.24",
    "@iconify-json/mdi": "^1.1.52",
    "@itshixun/qst-ui-system": "^0.1.0",
    "@voerkai18n/runtime": "^1.1.32",
    "@vueuse/core": "^10.1.2",
    "@vueuse/head": "^1.1.26",
    "axios": "^1.4.0",
    "element-plus": "^2.3.4",
    "lodash-es": "^4.17.21",
    "pinia": "^2.0.36",
    "pinia-plugin-persist": "^1.0.0",
    "vue": "^3.3.2",
    "vue-router": "^4.2.0"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.7",
    "@types/node": "^20.1.5",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitest/coverage-c8": "^0.31.0",
    "@vue/test-utils": "^2.3.2",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-vue": "^9.13.0",
    "jsdom": "^22.0.0",
    "lint-staged": "^13.2.2",
    "postcss": "^8.4.23",
    "postcss-html": "^1.5.0",
    "postcss-scss": "^4.0.6",
    "prettier": "^2.8.8",
    "sass": "^1.62.1",
    "simple-git-hooks": "^2.8.1",
    "stylelint": "^15.6.1",
    "stylelint-config-css-modules": "^4.2.0",
    "stylelint-config-recess-order": "^4.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard-scss": "^9.0.0",
    "stylelint-scss": "^5.0.0",
    "typescript": "^5.0.4",
    "unocss": "^0.51.13",
    "vite": "^4.3.6",
    "vitest": "^0.31.0",
    "vue-eslint-parser": "^9.3.0",
    "vue-tsc": "^1.6.5"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{vue,js,ts,jsx,tsx,json}": "eslint --ignore-path .gitignore --fix",
    "*.{vue,js,ts,jsx,tsx,md,json,scss,css,html}": "prettier . --write",
    "*.{scss,css,vue}": "stylelint --fix"
  }
}
